{
  "compilerOptions": {
    "target": "ES2022",
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "module": "NodeNext",
    "noEmit": true,
    "alwaysStrict": true,
    "rootDir": ".",
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "include": ["**/*"],
  "exclude": ["node_modules"]
}
